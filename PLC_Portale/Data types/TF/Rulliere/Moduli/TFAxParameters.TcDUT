  <DUT Name="TFAxParameters" Id="{d9576319-89f4-4248-9a7e-197637bf8439}">
    <Declaration><![CDATA[TYPE TFAxParameters :
STRUCT
	HomeDefine		: REAL	:= 0.0;	(* Home Define (Posizione di Azzeramento Encoder) *)
	HomePosition	: REAL	:= 0.0;	(* Home Position (Posizione di Home) *)
	Vel				: REAL	:= 0.0;	(* Velocità di funzionamento*)
	Acc				: REAL	:= 100.0; (* Accelerazione di funzionamento *)
	Dec				: REAL	:= 100.0; (* Decelerazione di funzionamento *)
	Jerk			: REAL  := 100.0; (* Jerk di funzionamento *) 
	MaxTorque		: REAL	:= 100.0; (* Coppia massima utilizzata se si effettua il controllo di coppia*)
	JOGVel			: REAL	:= 100.0; (* Velocità JOG *)
	JOGAcc			: REAL	:= 100.0; (* Accelerazione JOG *)
	JOGDec 			: REAL	:= 100.0; (* Decelerazione JOG *)
	MaxDeceleration : REAL  := 100.0; (* Massima Decelerazione *)
	MinPosition		: REAL	:= 0.0;
	MaxPosition		: REAL	:= 0.0;
	MaxVel		    : REAL  := 0.0; (* Massima velocità per gestire i jog manuali *)
	MaxAcc		    : REAL  := 0.0; (* Massima velocità per gestire i jog manuali *)
	MaxDec		    : REAL  := 0.0; (* Massima velocità per gestire i jog manuali *)
	ManSlowVel		: REAL	:= 0.0; (* Manual Slow Velocity (% of MaxVelocity) *)
	ManFastVel		: REAL	:= 0.0; (* Manual Fast Velocity (% of MaxVelocity) *)
	Quota_Destinazione	: LREAL  := 0.0; (*Posizione target da raggiungere*)
	HomeVelocity		: REAL  := 1.0; (* Velocità per ricerca sesore di Home *)
END_STRUCT
END_TYPE]]></Declaration>
