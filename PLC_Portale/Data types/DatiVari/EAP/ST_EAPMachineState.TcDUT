		(* Stato del comando in esecuzione *)
		CmdCase : UDINT;
		(* Errore generico della Macchina *)
		Error : UDINT;
		(* Errore specifico della Macchina *)
		ErrorExtra : UDINT;
		(* Macchina con potenza attiva *)
		PowerActive : BOOL;
		(* Macchina con Home eseguito *)
		Homed : BOOL;
		(* Macchina in automatico *)
		Auto : BOOL;
		(* Macchina in automatico e con ciclo di lavoro attivo -> sta lavorando *)
		Working : BOOL;
		(* Macchina accesso manuale in area segreta attivo *)
		ManAccesActive : BOOL;
		(* Macchina in Emergenza *)
		EmergencyOK : BOOL;
	END_STRUCT
END_TYPE]]></Declaration>
  </DUT>
