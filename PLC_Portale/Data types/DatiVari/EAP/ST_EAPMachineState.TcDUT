  <DUT Name="ST_EAPMachineState" Id="{2c4c196f-0f6c-4fc5-8656-d0e5b1b8a202}">
    <Declaration><![CDATA[TYPE ST_EAPMachineState : 
	STRUCT
		(* Comando in esecuzione *)
