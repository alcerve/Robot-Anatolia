		(*******************************************************************************************************************)						
									(* Deposito su LASTRA in cassa con rotazione piano = 90° *)
		(*******************************************************************************************************************)
		ELSE
			
			Mission^.XPick	:= WorkArea^[WADiPrelievo].X + WorkAreaOffset^[WADiPrelievo].X + 																																					(* Quota X del Cavaletto *)	
								Forming^[WorkAreaState^[WADiPrelievo].F].PackXP[NLastraDaPrelevare].X+	
								Pallet^[Forming^[WorkAreaState^[WADiPrelievo].F].Pallet].OFSDE_X90;
								
			Mission^.YPick := WorkArea^[WADiPrelievo].Y + WorkAreaOffset^[WADiPrelievo].Y + 																																				(* Quota Y del Cavaletto *)						
								Forming^[WorkAreaState^[WADiPrelievo].F].PackXP[NLastraDaPrelevare].Y + 																	(* Quota di deposito Y Ultima Lastra Depositata *)
