			Mission^.ZPick	:= WorkArea^[WADiPrelievo].Z + WorkAreaOffset^[WADiPrelievo].Z +
								Forming^[WorkAreaState^[WADiPrelievo].F].PackXP[1].Z +	
								Pallet^[Forming^[WorkAreaState^[WADiPrelievo].F].Pallet].OFSDE_Z90;
			Mission^.JPick	:= Forming^[WorkAreaState^[WADiPrelievo].F].PackXP[NLastraDaPrelevare].J +	
								Pallet^[Forming^[WorkAreaState^[WADiPrelievo].F].Pallet].OFSDE_J90;
		END_IF
		
