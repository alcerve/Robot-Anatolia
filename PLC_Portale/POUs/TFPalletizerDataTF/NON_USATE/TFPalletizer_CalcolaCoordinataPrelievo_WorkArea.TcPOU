								Pallet^[Forming^[WorkAreaState^[WADiPrelievo].F].Pallet].OFSDE_Z90;
								
			Mission^.JPick	:= Forming^[WorkAreaState^[WADiPrelievo].F].PackXP[NLastraDaPrelevare].J +	
